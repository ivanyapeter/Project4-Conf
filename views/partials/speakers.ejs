<section class="section-body" id="speakers">
	<div class="section-heading">
		<h1>Speakers</h1>
		<hr>
	</div>

	<% speakers.forEach(function(speaker){ %>
		<div class="speaker">
			<img src="<%= speaker.imgPath %>" alt="">
			<div class="description">
				<h3><%= speaker.name %></h3>
				<p><%= speaker.description %></p>
			</div>
		</div>
	<% }); %>
</section>
